.has_house{
  background-color: greenyellow;
  width: 300rpx;
  height: 300rpx;
  border-radius: 50%;
  padding: 80rpx;
  position: absolute;
  top: 100rpx;
  left: 30%;
  box-sizing: border-box;
}
.not_house{
  width: 300rpx;
  height: 300rpx;
  border-radius: 50%;
  padding: 80rpx;
  background-color: #f5f5f5;
  position: absolute;
  top: 500rpx;
  left: 30%;
  box-sizing: border-box;
}