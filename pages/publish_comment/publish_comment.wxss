Page{
  background: #f5f5f5;
  padding: 0;
  margin: 0;
}
.content{
  width: 90%;
  margin-left: 5%;
  border-radius: 25rpx;
  background-color: #ffffff;
  margin-top: 20rpx;
}
.item{
  width: 92%;
  margin-left: 4%;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-bottom: 1px solid rgb(206, 201, 201);
  padding-bottom: 20rpx;
}
.text{
  margin: 20rpx 0 0 20rpx;
}
.button{
  display: flex;
  justify-content: center;
}
.bottom{
  width: 300rpx;
  height: 90rpx;
  background-color: rgb(98, 235, 128);
  border-radius: 30rpx;
  margin-top: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}