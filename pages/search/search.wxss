.index_top{
  width: 85%;
  background-color: #ffffff;
  border-radius: 50rpx;
  height: 100rpx;
  margin: 15rpx 0 0 20rpx;
  display: flex;
  align-items: center;
}
.search{
  background-color: #ffffff;
  position: fixed;
  top: 0;
  z-index: 999;
}
.city_select{
  margin-left: 20rpx;
}
.tuijian{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 110rpx;
}
.tuijian_item{
  width: 100%;
  padding:8px 0 8px 55%;
  border-bottom: 1px solid rgb(238, 232, 232);
}
.list{
  margin-top: 110rpx;
  height: 100%;
}
.booklist{
  height: 200rpx;
  display: flex;
  flex-direction: row;
}
.book-img {
  
  width: 147rpx;
  height: 167rpx;
  box-shadow: -1px -1px 10rpx #bbb;
  margin-top: 8px;
  margin-left: 12px;
}
.book-info {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size: 28rpx;
  color:black;
  margin-left: 30rpx;
  margin-top: 8px;
}
.house_content {
  width: 400rpx;
  /* line-height: 60rpx; */
  display: block !important;
  margin-top: 6px;
  white-space: nowrap;
  font-size: 14px;
  overflow: hidden;
  text-overflow: ellipsis;
}
.rent_type{
  border-radius: 10rpx;
  color: white;
  background-color: rgb(108, 105, 252);
  padding: 6rpx 10rpx 0rpx 10rpx;
  margin-right: 15rpx;
  margin-top: 8rpx;
}